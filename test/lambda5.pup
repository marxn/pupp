def fff : function;
def ggg : function;
def ppp : function;

def b = (x : int) using(&fff,&ggg,&ppp)
{
	fff = () using(&x) { x = 200; };
	ggg = () using(&x) { x = 300; };
	ppp = () using(&x) { print x,NL; };
};

call b(100);
call ppp();
call fff();
call ppp();
call ggg();
call ppp();

call b(200);
call ppp();

return 0;

