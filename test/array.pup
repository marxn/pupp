def func = (args : set) using(&test, &test1, &test2, &test3): int
{
        def arr : int[10];
        def i = 0;

        for(i = 0; i < 10; i = i + 1)
        {
                arr[i] = i;
        }

	print arr,NL;

        if(true)
        {
                call test(arr);
                print arr,NL;

                call test1(arr);
                print arr,NL;

                call test2(arr[1]);
                print arr,NL;
        
                call test3(arr[1]);
                print arr,NL;
                sleep(1000000);
        }
        return 0;
};

def test = (x : int[])
{
        x[0] = 123;
};


def test1 = (&x : int[])
{
        x[0] = 123;
};


def test2 = (x : int)
{
        x = 456;
};


def test3 = (&x : int)
{
        x = 456;
};

return func(args);
