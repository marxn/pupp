def a = (x : int) : function
{
	return (y : int) : int{return x + y;};
};

def fff : function;
def ggg : function;
def ppp : function;

def b = (x : int)
{
	fff = () { x = 200; };
	ggg = () copy(x) { x = 300; };
	ppp = () { print x,NL; };
};

def main = (args : set) : int
{
	def con = 10;
	def f = a(con);
	con = 20;
	def g = a(con);
	print f(1),NL;
	print g(1),NL;

	call b(100);
	call ppp();
	call fff();
	call ppp();
	call ggg();
	call ppp();

	call b(200);
	call ppp();
};
