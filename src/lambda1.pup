def main = (args : set) : int
{
        def fun_array : function[10, 10];
        def i = 0;
        def j = 0;

        for(i = 0; i < 10; i = i + 1)
        {
            for(j = 0; j < 10; j = j + 1)
            {
                fun_array[i, j] = () using(i, j) : int { return i * j; };
            }
        }

        print fun_array[2, 4](), NL;
        return 0;
};

